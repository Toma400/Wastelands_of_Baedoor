{
  "multipart": [
    {
      "apply": {
        "model": "wobr:block/bamboo_panel_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "wobr:block/bamboo_panel_noside",
        "y": 270
      }
    }
  ]
}